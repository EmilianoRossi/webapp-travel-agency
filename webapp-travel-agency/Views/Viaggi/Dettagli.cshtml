@model Viaggio
<body>

    <main>

        <div class="card bg-dark text-white">
          <img class="card-img" src="@Model.Foto" alt="Card image">
          <div class="card-img-overlay">
            <h1 class="card-title">@Model.Nome</h1>
            <h2 class="card-text">@Model.Descrizione</h2>
            <h3 class="card-text">@Model.Prezzo €</h3>
          </div>
        </div>

    </main>

</body>